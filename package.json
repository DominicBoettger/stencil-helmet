{
  "name": "@stencil/helmet",
  "version": "0.2.6",
  "description": "Declaratively update the head from a Stencil app.",
  "main": "dist/stencil-helmet.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "start": "tsc --watch",
    "build": "tsc",
    "test": "jest",
    "prepublish": "npm run build"
  },
  "author": "Ionic Team",
  "license": "MIT",
  "homepage": "https://github.com/ionic-team/stencil-helmet",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-team/stencil-helmet.git"
  },
  "bugs": {
    "url": "https://github.com/ionic-team/stencil-helmet"
  },
  "devDependencies": {
    "@types/jest": "^22.1.1",
    "jest": "^22.1.4",
    "np": "^3.0.4",
    "ts-jest": "^23.0.1",
    "typescript": "^2.6.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
